// Rundungsfunktion
/** nf (zu rundende Zahl, Anzahl Kommastellen) */
nf(zahl,stellen) := N(Eval(Round(zahl*10^stellen)/10^stellen));
/** nf (Zahl) */
nf(zahl) := N(Eval(zahl));

// Löst numerisch Gleichungen. Äquivalent zu Newton.
nSolve(expr,var,initial,accuracy) := `(Newton(Eval(@expr),@var,initial,accuracy));
nSolve(expr,var,initial) := `(Newton(Eval(@expr),@var,initial,0.0000000001));
