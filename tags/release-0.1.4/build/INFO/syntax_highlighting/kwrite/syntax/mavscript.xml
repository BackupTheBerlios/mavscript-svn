    <contexts>
      <context name="default" attribute="Normal Text" lineEndContext="#stay">
        <Detect2Chars attribute="Comment" context="linecomment" char="/" char1="/"/>
        <Detect2Chars attribute="Comment" context="linecomment" char="/" char1="*"/>
        <Detect2Chars attribute="Highlight" context="mavscriptcommand" char="$" char1="m"/>
        <keyword attribute="Highlight" context="#stay" String="Keyword"/>
        <AnyChar attribute="Parens" context="#stay" String="{[(" beginRegion="brace"/>
        <AnyChar attribute="Parens" context="#stay" String="}])" endRegion="brace"/>
      </context>
      <context name="string" attribute="String" lineEndContext="#pop">
        <LineContinue attribute="String" context="#stay"/>
        <DetectChar attribute="String" context="#pop" char="&quot;"/>
      </context>
      <context name="linecomment" attribute="Comment" lineEndContext="#pop">
        <RegExpr attribute="Highlight" context="#stay" String="(FIXME|TODO)" />
      </context>
      <context name="mavscriptcommand" attribute="Keyword" lineEndContext="#pop">
        <StringDetect attribute="Highlight" context="#pop" String="$io"/>
        <Detect2Chars attribute="Highlight" context="#pop" char="$" char1="o"/>
        <Detect2Chars attribute="Highlight" context="#pop" char="$" char1="i"/>
        <Detect2Chars attribute="Highlight" context="#pop" char="$" char1="n"/>
        <AnyChar attribute="Parens" context="#stay" String="{[(" beginRegion="brace"/>
        <AnyChar attribute="Parens" context="#stay" String="}])" endRegion="brace"/>
        <Float attribute="Float" context="#stay"/>
        <Int attribute="Decimal" context="#stay"/>
        <keyword attribute="FunctionDiscrete" context="#stay" String="init"/>
        <keyword attribute="Function" context="#stay" String="yacas"/>
        <keyword attribute="Function" context="#stay" String="beanshell"/>
        <DetectChar attribute="String" context="string" char="&quot;"/>
      </context>
      
    </contexts>
